{
  "kind": "collectionType",
  "collectionName": "evenements",
  "info": {
    "singularName": "evenement",
    "pluralName": "evenements",
    "displayName": "Évènement"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nom": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "commune": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::commune.commune"
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Coordonnees": {
      "type": "customField",
      "customField": "plugin::geodata.geojson",
      "options": {
        "info": true
      }
    },
    "Tel": {
      "type": "string"
    },
    "Liens": {
      "type": "component",
      "component": "partager.lien-image",
      "repeatable": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
