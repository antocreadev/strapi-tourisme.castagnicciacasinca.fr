{
  "kind": "collectionType",
  "collectionName": "sejourners",
  "info": {
    "singularName": "sejourner",
    "pluralName": "sejourners",
    "displayName": "Sejourner"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titre": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "commune": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::commune.commune"
    },
    "Coordonnees": {
      "type": "customField",
      "customField": "plugin::geodata.geojson",
      "options": {
        "info": true
      }
    },
    "type_sejourner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::type-sejourner.type-sejourner"
    },
    "Contact": {
      "type": "string"
    },
    "liens": {
      "type": "component",
      "component": "partager.lien-image",
      "repeatable": true
    },
    "EtablissementCharteNote": {
      "type": "integer",
      "min": 0,
      "max": 3
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
